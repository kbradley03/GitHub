[32m+ 			$("#ajax-contact-form").submit(function() {[m
[32m+ 				var str = $(this).serialize();[m
[32m+ 				$.ajax({[m
[32m+ 					type: "POST",[m
[32m+ 					url: "contact_form/contact_process.php",[m
[32m+ 					data: str,[m
[32m+ 					success: function(msg) {[m
[32m+ 						// Message Sent - Show the 'Thank You' message and hide the form[m
[32m+ 						if(msg == 'OK') {[m
[32m+ 							result = '<div class="notification_ok">Your message has been sent. Thank you!</div>';[m
[32m+ 							$("#fields").hide();[m
[32m+ 						} else {[m
[32m+ 							result = msg;[m
[32m+ 						}[m
[32m+ 						$('#note').html(result);[m
[32m+ 					}[m
[32m+ 				});[m
[32m+ 				return false;[m
[32m+ 			});[m
[32m+ 		});[m
